{
    "name": "Pres2",
    "version": "1.0.0",
    "description": "Presentation Assistant",
    "manifest_version": 3,
    "author": "Jasmine, Gabe",

    "permissions": [
        "activeTab",
        "contextMenus",
        "storage",
        "tabs",
        "scripting"
    ],
    "optional_permissions": [
        "topSites"
    ],
    "host_permissions": [
            "http://*/*",
            "https://*/*"
            "https://server.com/*"

    ],
    "action": {
      "default_popup": "index.html",
      "default_title": "Pres2"
    },
    "background": {
    "service_worker": "background.js"
    },
    "icons": {
      "512": "icons/icon.png"
    },
}

  