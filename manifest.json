{
    "name": "Pres2",
    "version": "1.0.0",
    "description": "Presentation Assistant",
    "manifest_version": 3,
    "author": "Jasmine, Gabe",

    "permissions": [
        "activeTab",
        "contextMenus",
        "storage",
        "activeTab",
        "tabs"
    ],
    "optional_permissions": [
        "topSites"
    ],
    "host_permissions": [
            "http://*/*",
            "https://*/*"
    ],
    

    "action": {
      "default_popup": "index.html"
    },

    "background": {
    "service_worker": "background.js"
    }

}

  